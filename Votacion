#include "stdafx.h"
#include <iostream>
#include "conio.h"

using namespace std;

void main ()
{
	int edad;
	cout<<"Ingrese edad: ";
	cin>>edad;
	if ( edad>=18)

		cout<<"Habilitado para votar"<<endl;
	else
		cout<<"No puede votar";

	getch();
}
